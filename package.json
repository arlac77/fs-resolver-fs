{
  "name": "fs-resolver-fs",
  "version": "0.0.0-semantic-release",
  "description": "resolves file urls",
  "main": "dist/FileScheme.js",
  "module": "src/FileScheme.js",
  "scripts": {
    "cover": "nyc ava",
    "test": "ava",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/FileScheme.js -c -- src/FileScheme.js",
    "pretest": "rollup -c rollup.config.test.js",
    "posttest": "markdown-doctest",
    "precover": "rollup -c rollup.config.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/fs-resolver-fs.git"
  },
  "keywords": [
    "url",
    "resolver",
    "fs-resolver-fs"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/fs-resolver-fs/issues"
  },
  "homepage": "https://github.com/arlac77/fs-resolver-fs#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^10.3.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "semantic-release": "^6.3.6",
    "xo": "^0.19.0"
  },
  "dependencies": {
    "url-resolver-fs": "^3.0.0"
  },
  "engines": {
    "node": ">=7.10.0"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "tests/build/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "tests/build/bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}